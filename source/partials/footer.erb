<div class="footer">
  <div class="div-50 footer">
    <div data-w-id="546b067b-e6c6-4893-1b34-49d1e5dba302" style="opacity:0" class="div-50 lieu">
      <% dato.sections.each do |section1| %>
      <% if section1.titre == "Localisation" || section1.titre == "Location" %>
      <div id="<%= section1.slug %>" class="subtitle"><%= section1.sous_titre %></div>
      <div class="title lieu"><%= section1.slogan %></div>
      <%= section1.description %>

      <% if section1.lien_externe %>
      <a href="<%= section1.lien %>" target="_blank" class="button yellow w-inline-block">
        <div><%= section1.titre_lien %></div>
      </a>
      <% end %>
      <% end %>
      <% end %>
    </div>
    <div data-w-id="212e6b4f-0784-0828-4a11-7597ded9cd52" style="opacity:0" class="div-50 lieu">
      <% dato.sections.each do |section1| %>
      <% if section1.titre == "Date" %>
      <div class="subtitle"><%= section1.sous_titre %></div>
      <div class="title lieu"><%= section1.slogan %></div>
      <%= section1.description %>

      <% if section1.lien_externe %>
      <a href="<%= section1.lien %>" target="_blank" class="button yellow w-inline-block">
        <div><%= section1.titre_lien %></div>
      </a>
      <% end %>
      <% end %>
      <% end %>
    </div>
    <div class="div-50 lieu full">
      <% dato.sections.each do |section1| %>
      <% if section1.titre == "Informations" %>
      <div class="subtitle"><strong><%= section1.sous_titre %></strong></div>
      <div class="title lieu"><%= section1.slogan %></div>
      <div><%= section1.description %></div>
      <% end %>
      <% end %>
    </div>
  </div>
  <div class="div-map">
    <div class="map w-embed w-iframe"><iframe src="https://snazzymaps.com/embed/111284" width="100%" height="100%" style="border:none;"></iframe></div>
  </div>
  <div class="div-credits">
    <div>© Colloque Vecteur 2018, Tous droits réservés</div>
  </div>
</div>
</div>
