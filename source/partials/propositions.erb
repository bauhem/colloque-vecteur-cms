<% dato.sections.each do |section1| %>
<% if section1.slug == "appel" || section1.slug == "call" %>
<% if section1.actif %>
<div id="<%= section1.slug %>" class="section white">
  <div class="div-about-content reverse">
    <div data-w-id="d24c09f2-a185-16e4-d631-9dfaa4c00946" style="opacity:0" class="div-50 text">
      <div class="subtitle"><%= section1.sous_titre %></div>
      <div class="title yellow"><%= section1.slogan %></div>
      <div><%= section1.description %></div>
      <div><%= section1.description_2 %></div>
      <div><%= section1.description_3 %></div>
      <% if section1.lien_externe %>
      <a href="<%= section1.lien %>" target="_blank" class="button white w-inline-block">
        <div><%= section1.titre_lien %></div>
      </a>
      <% end %>
    </div>
    <div data-w-id="d24c09f2-a185-16e4-d631-9dfaa4c00955" style="opacity:0" class="div-50 pictures-reverse">
      <div data-w-id="d24c09f2-a185-16e4-d631-9dfaa4c00956" style="opacity:0" class="div-picture fourth" data-ix="div-picture-moving-loop"><img src="/images/FP-CIDCO_2017-20.jpg" alt=""></div>
      <div data-w-id="d24c09f2-a185-16e4-d631-9dfaa4c00958" style="opacity:0" class="div-picture fifth" data-ix="div-picture-moving-loop"><img src="/images/FP-CIDCO_2017-08.jpg" alt=""></div>
      <div data-w-id="d24c09f2-a185-16e4-d631-9dfaa4c0095a" style="opacity:0" class="div-picture sixth" data-ix="div-picture-moving-loop"><img src="/images/FP-CIDCO_2017-03.jpg" alt=""></div>
    </div>
  </div>
  <div class="div-background"><img src="/images/grey.png" srcset="/images/grey-p-500.png 500w, /images/grey-p-800.png 800w, /images/grey-p-1080.png 1080w, /images/grey-p-1600.png 1600w, /images/grey-p-2000.png 2000w, /images/grey-p-2600.png 2600w, /images/grey.png 3812w" sizes="100vw" alt="" class="background-image grey"></div>
</div>
<% end %>
<% end %>
<% end %>
