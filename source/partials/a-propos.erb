<% dato.sections.each do |section1| %>
<% if section1.slug == "a-propos" || section1.slug == "about" %>
<div id="<%= section1.titre.downcase %>"
<div class="sections-container">
  <div id="a-propos" class="section yellow">
    <div class="div-about-content">
      <div data-w-id="1a67e99b-530c-52f3-e7ee-d319df83d861" style="opacity:0" class="div-50 text">
        <div class="subtitle"><%= section1.sous_titre %></div>
        <div class="title"><%= section1.slogan %></div>
        <div><%= section1.description %></div>

        <% if section1.lien_externe %>
        <a href="<%= section1.lien %>" target="_blank" class="button margin w-inline-block">
          <div><%= section1.titre_lien %></div>
        </a>
        <% end %>

      </div>
      <div class="div-50 pictures">
        <div data-w-id="621f0338-cac7-41f0-6890-b0458dfb52de" style="opacity:0" class="div-picture first" data-ix="div-picture-moving-loop"><img src="/images/seventh.jpg" srcset="/images/seventh-p-500.jpeg 500w, /images/seventh-p-800.jpeg 800w, /images/seventh.jpg 1000w" sizes="(max-width: 479px) 55vw, (max-width: 767px) 57vw, (max-width: 991px) 54vw, (max-width: 3703px) 27vw, 1000px" alt=""></div>
        <div data-w-id="31363c48-fc80-3396-c0b6-ec6c64434dcd" style="opacity:0" class="div-picture third" data-ix="div-picture-moving-loop"><img src="/images/FP-CIDCO_2017-13.jpg" srcset="/images/FP-CIDCO_2017-13-p-1080.jpeg 1080w, /images/FP-CIDCO_2017-13-p-1600.jpeg 1600w, /images/FP-CIDCO_2017-13-p-2000.jpeg 2000w, /images/FP-CIDCO_2017-13-p-2600.jpeg 2600w, /images/FP-CIDCO_2017-13-p-3200.jpeg 3200w, /images/FP-CIDCO_2017-13.jpg 3596w" sizes="(max-width: 479px) 41vw, (max-width: 767px) 43vw, (max-width: 991px) 41vw, 20vw" alt=""></div>
        <div data-w-id="fbb62486-d9bb-c86a-3c5f-079c42d9a472" style="opacity:0" class="div-picture second" data-ix="div-picture-moving-loop"><img src="/images/FP-CIDCO_2017-28.jpg" srcset="/images/FP-CIDCO_2017-28-p-500.jpeg 500w, /images/FP-CIDCO_2017-28-p-800.jpeg 800w, /images/FP-CIDCO_2017-28.jpg 863w" sizes="(max-width: 479px) 64vw, (max-width: 767px) 66vw, (max-width: 991px) 63vw, 32vw" alt=""></div>
      </div>
    </div>
    <div class="div-background"><img src="/images/white.png" srcset="/images/white-p-500.png 500w, /images/white-p-800.png 800w, /images/white-p-1080.png 1080w, /images/white-p-1600.png 1600w, /images/white-p-2000.png 2000w, /images/white-p-2600.png 2600w, /images/white-p-3200.png 3200w, /images/white.png 3812w" sizes="100vw" alt="" class="background-image"></div>
  </div>
<% end %>

<% end %>
